[
  {
    "kernelSource": {
      "name": "AnymoreVayu",
      "repo": "https://github.com/AnymoreProject/android_kernel_vayu",
      "branch": "16-ksu", // Puedes cambiarlo a "16", "16-miui", etc.
      "device": "vayu",
      "defconfig": "vayu_defconfig"
    },
    "toolchains": [
      {
        "name": "aosp-clang",
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master/clang-r510928.tar.gz",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,      // Crea el zip flasheable
      "release": false  // No crea un "Release" en GitHub autom√°ticamente
    },
    "enableKernelSU": true, // Activa el parcheo de KernelSU
    "enableLXC": false
  }
]
