[
  {
    "kernelSource": {
      "name": "AnymoreVayu",
      "repo": "https://github.com/AnymoreProject/android_kernel_vayu",
      "branch": "16-ksu",
      "device": "vayu",
      "defconfig": "vayu_defconfig"
    },
    "toolchains": [
      {
        "name": "aosp-clang",
        "url": "https://github.com/AOSP-Clang/Main-Clang/releases/download/17.0.2-r498229b/17.0.2-r498229b.tar.gz",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
