[
  {
    "kernelSource": {
      "name": "Tejas-JuanVayuKernel",
      "repo": "https://github.com/tejas101k/kernel_xiaomi_vayu",
      "branch": "sixteen",
      "device": "vayu",
      "defconfig": "vendor/vayu_defconfig"
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "LD": "ld.lld",
        "LLVM": "1",
        "LLVM_IAS": "1",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "STRIP": "llvm-strip"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
