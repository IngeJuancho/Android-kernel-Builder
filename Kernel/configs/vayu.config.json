[
  {
    "kernelSource": {
      "name": "AnymoreVayu",
      "repo": "https://github.com/AnymoreProject/android_kernel_vayu",
      "branch": "16-ksu",
      "device": "vayu",
      "defconfig": "vayu_defconfig"
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "LD": "ld.lld",
        "LLVM": "1",
        "LLVM_IAS": "1"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
